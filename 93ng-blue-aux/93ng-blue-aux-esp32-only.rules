
(rules PCB 93ng-blue-aux-esp32-only
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 99)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_450:300_um"
    (shape
      (circle F.Cu 450.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 450.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_450:300_um" "Via[0-1]_450:300_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_450:300_um-kicad_default" "Via[0-1]_450:300_um" default
  )
  (via_rule
    default "Via[0-1]_450:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C1-Pad1)" GND 3.3V "Net-(C4-Pad1)" "Net-(C4-Pad2)" "SW_CAN" "MC33_TX" "MC33_RX"
    "Net-(R9-Pad2)" "MC33_MODE0" "MC33_MODE1" "ESP32_EN" "Net-(D2-Pad2)" "USB_VBUS" "Net-(D4-Pad2)" "Net-(J1-Pad2)"
    "ESP32_RX" "ESP32_TX" "USB_CC1" "USB_D+" "USB_D-" "USB_CC2" RTS "Net-(Q1-Pad2)"
    "Net-(Q2-Pad2)" "ESP32_BOOT" DTR "Net-(R3-Pad1)" "Net-(R11-Pad1)" "Net-(R12-Pad2)"
    (clearance_class default)
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)